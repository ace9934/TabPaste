{
    "name": "TabPaste",
    "version": "1.0",
    "description": "Quickly reopen tabs from TabCopy's text",
    "permissions": ["activeTab"],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/paste16.png",
            "32": "images/paste32.png",
            "48": "images/paste48.png",
            "128": "images/paste128.png"
        }
    },
    "icons": {
        "16": "images/paste16.png",
        "32": "images/paste32.png",
        "48": "images/paste48.png",
        "128": "images/paste128.png"
    },
    "manifest_version": 2
}
